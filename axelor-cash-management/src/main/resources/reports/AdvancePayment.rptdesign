            <xml-property name="queryText"><![CDATA[select AdvancePayment.date_record as DateRecord,
AdvancePayment.fill_sum as TotalSum,
AdvancePayment.fill_text as TotalText,
AdvancePaymentLine.seq_number as Number,
AdvancePaymentLine.advance_sum as AdvanceSum,
AdvancePaymentLine.advance_text as AdvanceText,
Employee.full_name as EmpName,
Project.name as ProjectName



from management_advance_payment_cash as AdvancePayment
left outer join management_advance_payment_cash_line as AdvancePaymentLine on (AdvancePayment.id = AdvancePaymentLine.advance_payment_cash)
left outer join hr_employee_ru as Employee on (AdvancePaymentLine.employee_ru = Employee.id)
left outer join project_project_ru as Project on (AdvancePayment.project = Project.id)
where AdvancePayment.id = '1';
]]></xml-property>
