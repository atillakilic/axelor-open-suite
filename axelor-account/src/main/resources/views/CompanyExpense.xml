<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid name="company-expense-grid" title="Company Expense"
    model="com.axelor.apps.account.db.CompanyExpense">
    <field name="name"/>
    <field name="code"/>
    <field name="showOnReport"/>
    <field name="expensePeriod"/>
  </grid>

  <form name="company-expense-form" title="Company Expense"
    model="com.axelor.apps.account.db.CompanyExpense">
    <panel name="mainPanel">
      <field name="name"/>
      <field name="code"/>
      <field name="showOnReport"/>
      <field name="expensePeriod"/>
    </panel>
  </form>


</object-views>
