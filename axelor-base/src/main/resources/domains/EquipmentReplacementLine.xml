<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="EquipmentReplacementLine" lang="java" cacheable="true">

    <many-to-one name="replacementName" ref="com.axelor.apps.base.db.Product"
      title="Replacement Name"/>
    <decimal name="quentity" title="Quantity"/>
    <string name="description" title="Description"/>

    <many-to-one name="product" ref="com.axelor.apps.base.db.Product"/>

  </entity>

</domain-models>
