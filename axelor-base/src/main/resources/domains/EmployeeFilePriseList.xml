<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="EmployeeFilePriseList" cacheable="true">
    <date name="updatedDate" namecolumn="true" title="Updated Date"/>
    <decimal name="price" title="Price"/>
    <string name="description" title="Description"/>

    <many-to-one name="employeeFile" ref="com.axelor.apps.base.db.FileType"/>
  </entity>

</domain-models>
