<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid name="employee-file-prise-list-grid" title="Prise List"
    model="com.axelor.apps.base.db.EmployeeFilePriseList">
    <field name="updatedDate"/>
    <field name="price"/>
    <field name="description"/>
  </grid>

  <form name="employee-file-prise-list-form" title="Prise List"
    model="com.axelor.apps.base.db.EmployeeFilePriseList">
    <panel name="mainPanel">
      <field name="updatedDate" colSpan="6" required="true"/>
      <field name="price" colSpan="6" required="true"/>
      <field name="description" colSpan="12"/>
    </panel>
  </form>


</object-views>
