<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid name="project-category-grid" title="ProjectCategory"
    model="com.axelor.apps.project.db.ProjectCategory">
    <field name="name"/>
    <field name="code"/>
  </grid>

  <form name="project-category-form" title="ProjectCategory"
    model="com.axelor.apps.project.db.ProjectCategory" width="large">
    <panel name="mainPanel">
      <field name="name"/>
      <field name="code"/>
    </panel>
  </form>

</object-views>