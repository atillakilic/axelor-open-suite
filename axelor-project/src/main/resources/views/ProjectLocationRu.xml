<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid name="project-location-ru-grid" title="Project Location"
    model="com.axelor.apps.project.db.ProjectLocaltionRu" orderBy="name">
    <field name="name"/>
    <field name="code"/>
    <field name="isActive"/>
    <field name="note"/>
  </grid>

  <form name="project-location-ru-form" title="Project Location"
    model="com.axelor.apps.project.db.ProjectLocaltionRu" onSave="">
    <panel name="informationPanel">
      <field name="name"/>
      <field name="code"/>
      <field name="isActive"/>
      <field name="note"/>
    </panel>
  </form>

</object-views>