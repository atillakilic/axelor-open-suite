<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid name="project-validator-ru-grid" title="Project Validator"
    model="com.axelor.apps.project.db.ProjectValidatorRu" orderBy="name">
    <field name="name"/>
    <field name="projectWorker"/>
    <field name="departmentRu"/>
    <field name="isActive"/>
    <field name="note"/>
  </grid>

  <form name="project-validator-ru-form" title="Project Validator"
    model="com.axelor.apps.project.db.ProjectValidatorRu" onSave="">
    <panel name="informationPanel">
      <field name="name"/>
      <field name="projectWorker"/>
      <field name="departmentRu"/>
      <field name="isActive"/>
      <field name="note"/>
    </panel>
  </form>

</object-views>