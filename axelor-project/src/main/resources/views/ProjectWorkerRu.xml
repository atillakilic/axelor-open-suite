<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid name="project-worker-ru-grid" title="Project Worker"
    model="com.axelor.apps.project.db.ProjectWorkerRu" orderBy="name">
    <field name="name"/>
    <field name="departmentRu"/>
    <field name="salary"/>
    <field name="workingDay"/>
    <field name="totalWorkingDay"/>
  </grid>

  <form name="project-worker-ru-form" title="Project Worker"
    model="com.axelor.apps.project.db.ProjectWorkerRu" onSave="">
    <panel name="informationPanel">
      <field name="name"/>
      <field name="departmentRu"/>
      <field name="salary"/>
      <field name="workingDay"/>
      <field name="totalWorkingDay"/>
    </panel>
  </form>

</object-views>