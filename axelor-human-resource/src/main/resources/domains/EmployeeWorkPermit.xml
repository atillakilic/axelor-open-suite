<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="human-resource" package="com.axelor.apps.hr.db"/>

  <entity name="EmployeeWorkPermit" cacheable="true">

    <!-- WorkPermit details -->
    <many-to-one name="workPermitMetaFile" title="File"
      ref="com.axelor.apps.hr.db.EmployeeFile"/>
    <string name="workPermitSeriesNo" title="Series No"/>
    <string name="workPermitDescription" title="Description"/>


    <many-to-one name="employeeHr" ref="com.axelor.apps.hr.db.EmployeeHr"
      title="Employee"/>

  </entity>

</domain-models>

