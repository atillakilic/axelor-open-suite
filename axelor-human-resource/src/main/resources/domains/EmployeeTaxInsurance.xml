<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="human-resource" package="com.axelor.apps.hr.db"/>

  <entity name="EmployeeTaxInsurance" cacheable="true">
    <date name="innDateOfRegistration" title="Inn Date of registration"/>
    <string name="innInnNo" title="Inn No"/>
    <string name="innTaxAuthority" title="Inn Tax authority"/>

    <string name="snilsNo" title="SNILS No"/>
    <date name="snilsDateOfRegistration" title="SNILS Date of registration"/>

    <string name="voSeries" title="Series"/>
    <string name="voNumber" title="Number"/>
    <string name="voIssuedBy" title="Issued by whom"/>
    <string name="voPeriodFrom" title="Period from"/>
    <string name="voPeriodTo" title="Period up to"/>
    <string name="voTerritory" title="Territory"/>
    <string name="voVmiMmi" title="VMI or MMI"/>

    <many-to-one name="employeeHr" ref="com.axelor.apps.hr.db.EmployeeHr"
      title="Employee"/>
  </entity>

</domain-models>

