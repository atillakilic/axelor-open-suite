<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="human-resource" package="com.axelor.apps.hr.db"/>

  <entity name="EmployeeRvp" cacheable="true">

    <many-to-one name="empRvpFile" title="File" ref="com.axelor.apps.hr.db.EmployeeFile"/>
    <integer name="rvpType" title="Type" selection="hr.employee.rvp.type.select"/>
    <date name="startDate" title="Validity period start date"/>
    <date name="expirationDate" title="Expiration date"/>
    <date name="effectiveDate" title="Effective date"/>
    <string name="serial" title="Serial"/>
    <string name="seriesNo" title="Series No"/>
    <string name="issuingAuthority" title="Issuing authority"/>
    <string name="descriptons" title="Descriptons"/>

    <many-to-one name="employeeHr" ref="com.axelor.apps.hr.db.EmployeeHr"
      title="Employee"/>

  </entity>

</domain-models>

