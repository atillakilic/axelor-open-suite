<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="WeeklyPlanning" cacheable="true">
    <integer name="eventType" title="Type" selection="events.planning.type"/>
    <decimal name="overTimeIgnore" title="Overtime to be ignored"/>
    <many-to-one name="employee" title="Employee" ref="com.axelor.apps.hr.db.EmployeeHr"/>
    <many-to-one name="department" title="Department"
      ref="com.axelor.apps.base.db.CompanyDepartment"/>
    <string name="description" title="Description"/>
  </entity>

</domain-models>