<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="human-resource" package="com.axelor.apps.hr.db"/>

  <entity name="EmployeeNotCameLine" cacheable="true">
    <many-to-one name="employeeHr" ref="com.axelor.apps.hr.db.EmployeeHr"
      title="Employee"/>
    <many-to-one name="notCameReason" ref="com.axelor.apps.hr.db.EmployeeNotCameReason"
      title="Reason"/>
    <string name="comments" title="Comments"/>

    <many-to-one name="employeeNotCame" ref="com.axelor.apps.hr.db.EmployeeNotCame"/>
  </entity>

</domain-models>

