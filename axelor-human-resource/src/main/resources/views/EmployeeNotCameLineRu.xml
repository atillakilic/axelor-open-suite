<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">

  <grid name="hr-employee-not-came-line-ru-grid" title="Employee Not Came" editable="true"
    model="com.axelor.apps.hr.db.EmployeeNotCameLineRu">
    <field name="employee"/>
    <field name="notCameReason"/>
    <field name="workArea"/>
    <field name="comments"/>
  </grid>

  <form name="hr-employee-not-came-line-ru-form" title="Employee Not Came"
    model="com.axelor.apps.hr.db.EmployeeNotCameLineRu" width="large">
    <panel name="mainPanel" showTitle="false" title="Main Panel">
      <field name="employee"/>
      <field name="notCameReason"/>
      <field name="workArea"/>
      <field name="comments"/>
    </panel>
  </form>

</object-views>