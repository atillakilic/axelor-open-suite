<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">

  <grid name="hr-employee-education-grid" title="Employee Education"
    model="com.axelor.apps.hr.db.EmployeeEducation">
    <field name="graduateSchoolLevel" title="Graduate school level"/>
    <field name="graduateSchoolName" title="Graduate school name"/>
    <field name="score" title="Score"/>
    <field name="descriptons" title="Descriptons"/>
  </grid>

  <form name="hr-employee-education-form" title="Employee Education"
    model="com.axelor.apps.hr.db.EmployeeEducation" width="large">
    <panel name="employeeEducation" showTitle="true" title="Employee Education">
      <field name="graduateSchoolLevel" title="Graduate school level"/>
      <field name="graduateSchoolName" title="Graduate school name"/>
      <field name="score" title="Score"/>
      <field name="descriptons" title="Descriptons"/>
    </panel>

  </form>
</object-views>