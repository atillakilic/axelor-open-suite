<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">

  <grid name="hr-employee-company-expences-type-line-ru-grid" editable="true"
    title="Company Expences Type Line" model="com.axelor.apps.hr.db.CompanyExpencesTypeLineRu">
    <field name="expenseDate"/>
    <field name="expencePrice"/>
    <field name="typeStatus"/>
  </grid>

  <form name="hr-employee-company-expences-type-line-ru-form"
    title="Company Expences Type Line" model="com.axelor.apps.hr.db.CompanyExpencesTypeLineRu"
    width="large">
    <panel name="mainPanel" showTitle="false" title="Main Panel">
      <field name="expenseDate"/>
      <field name="expencePrice"/>
      <field name="typeStatus"/>
    </panel>
  </form>

</object-views>