<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="sale" package="com.axelor.apps.sale.db"/>

  <entity name="SaleorderExpenseLine" lang="java">

    <many-to-one name="expenseType" ref="com.axelor.apps.project.db.ProjectCategory"
      title="Type"/>
    <decimal name="period" title="Period"/>
    <decimal name="piece" title="Piece"/>
    <decimal name="price" title="Price"/>
    <decimal name="totalPrice" title="Total Price"/>
    <string name="description" title="Description"/>

    <many-to-one name="saleOrder" ref="com.axelor.apps.sale.db.SaleOrder"
      title="Sale Order"/>

  </entity>

</domain-models>
